# Development Environment Variables for Frontend
# Copy to .env.local for local development

# Domain Configuration
NEXT_PUBLIC_DOMAIN=localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:5000
NEXT_PUBLIC_FRONTEND_URL=http://localhost:3000

# NextAuth Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=development-secret-key-change-in-production

# API Configuration (Development keys)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-test-key
NEXT_PUBLIC_RAZORPAY_KEY_ID=rzp_test_your-razorpay-test-key

# Features
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_PWA=false

# Environment
NODE_ENV=development